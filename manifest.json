{
  "name": "BlindFold",
  "version": "1.0.0",
  "description": "Hides NSFW images that uncover if hovered over",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "BlindFold"
  },

  "permissions": [
    "<all_urls>"
  ],

  "background":{
    "scripts": ["background.js","jquery-1.12.2.min.js"],
    "persistent": true
  },
  "manifest_version": 2
}
